.phpunit-phpstan-dba-mysqli.cache linguist-generated=true
.phpunit-phpstan-dba-mysql.cache linguist-generated=true
.phpunit-phpstan-dba-pgsql.cache linguist-generated=true

# Exclude non-essential files from dist
/tests export-ignore
